ackage calculadora;

public class Calculadora {


    public int somar (int a, int b) {
            return a + b;
        }

    }
    
package calculadora;

public class CalculadoraTestes {
    public static void main (String [] args ) {
        Calculadora calc = new Calculadora ();
        int soma = calc.somar(3,7);
        System.out.println(soma);
    }
    
}
